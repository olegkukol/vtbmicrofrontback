{
  "name": "vacantions_backend",
  "version": "0.0.1",
  "private": true,
  "author": "akishin@dev.vtb.ru",
  "repository": "https://github.com/frontmaker/vacations_backend.git",
  "scripts": {
    "build": "docker-compose up -d --force-recreate && npm start",
    "start": "tsc && node ./dist/server.js",
    "dev": "./bin/dev.sh",
    "compile": "prisma generate && tsc",
    "dev-server": "cross-env NODE_ENV=development nodemon",
    "ts-node-server": "ts-node src/server.ts",
    "seed": "prisma db seed --preview-feature",
    "prisma:generate": "prisma generate",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "lint:fix": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix"
  },
  "dependencies": {
    "@prisma/client": "2.21.2",
    "bcrypt": "^5.0.1",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "connect-redis": "4.0.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "joi": "17.4.0",
    "lodash": "^4.17.21",
    "pg": "8.6.0",
    "prisma": "2.21.2",
    "redis": "^3.1.2",
    "uuid": "8.3.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.1",
    "@types/compression": "1.7.0",
    "@types/connect-redis": "0.0.10",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "4.17.11",
    "@types/express-session": "1.17.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "14.14.41",
    "@types/pg": "7.14.11",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "cross-env": "^7.0.3",
    "eslint": "7.24.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
